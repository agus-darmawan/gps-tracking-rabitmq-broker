  "product_version": "3.12.0",
  "users": [
    {
      "name": "admin",
      "password_hash": "oOXxzPZWXYKaACHs2jYlhGdVtjdxKaLgqoQPvzO14GAdcP+E",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator",
      "limits": {}
    },
    {
      "name": "gps_device",
      "password_hash": "gps_secure_2024_hash",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "gps_device",
      "limits": {}
    },
    {
      "name": "backend_service",
      "password_hash": "backend_secure_2024_hash",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "backend_service",
      "limits": {}
    }
  ],
  "vhosts": [
    {
      "name": "/",
      "description": "Default virtual host for vehicle rental system",
      "metadata": {
        "description": "Main vhost for all vehicle rental operations",
        "tags": ["vehicle", "rental", "tracking"]
      }
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "gps_device",
      "vhost": "/",
      "configure": "",
      "write": "^(vehicle\\.realtime\\.|vehicle\\.report\\.).*",
      "read": "^vehicle\\.control\\..*"
    },
    {
      "user": "backend_service",
      "vhost": "/",
      "configure": "",
      "write": "^vehicle\\.control\\..*",
      "read": "^(vehicle\\.realtime\\.|vehicle\\.report\\.|vehicle\\.alerts).*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "vehicle-rental-cluster"
    },
    {
      "name": "internal_cluster_id",
      "value": "vehicle-cluster-2024"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "vehicle-ha-policy",
      "pattern": "vehicle.*",
      "apply-to": "queues",
      "definition": {
        "ha-mode": "exactly",
        "ha-params": 2,
        "ha-sync-mode": "automatic",
        "message-ttl": 86400000,
        "max-length": 100000,
        "overflow": "reject-publish"
      },
      "priority": 2
    },
    {
      "vhost": "/",
      "name": "realtime-policy",
      "pattern": "vehicle\\.realtime\\..*",
      "apply-to": "queues",
      "definition": {
        "message-ttl": 3600000,
        "max-length": 200000,
        "overflow": "drop-head",
        "expires": 7200000
      },
      "priority": 3
    },
    {
      "vhost": "/",
      "name": "dlq-policy",
      "pattern": ".*",
      "apply-to": "queues",
      "definition": {
        "dead-letter-exchange": "vehicle.dlx",
        "dead-letter-routing-key": "failed"
      },
      "priority": 0
    }
  ],
  "queues": [
    {
      "name": "vehicle.control.start_rental",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000,
        "x-queue-type": "classic",
        "x-dead-letter-exchange": "vehicle.dlx",
        "x-queue-description": "Commands to start vehicle rental sessions"
      }
    },
    {
      "name": "vehicle.control.end_rental",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000,
        "x-dead-letter-exchange": "vehicle.dlx",
        "x-queue-description": "Commands to end vehicle rental sessions"
      }
    },
    {
      "name": "vehicle.control.kill_engine",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 1800000,
        "x-max-length": 5000,
        "x-dead-letter-exchange": "vehicle.dlx",
        "x-queue-description": "Emergency engine kill commands"
      }
    },
    {
      "name": "vehicle.control.unlock",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 1800000,
        "x-max-length": 8000,
