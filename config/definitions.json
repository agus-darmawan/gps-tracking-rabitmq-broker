{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "users": [],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "rental-cluster"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "vehicle-ha-policy",
      "pattern": "vehicle.*",
      "apply-to": "queues",
      "definition": {
        "ha-mode": "exactly",
        "ha-params": 2,
        "ha-sync-mode": "automatic",
        "message-ttl": 86400000,
        "max-length": 100000,
        "overflow": "reject-publish"
      },
      "priority": 1
    },
    {
      "vhost": "/",
      "name": "dlq-policy",
      "pattern": ".*",
      "apply-to": "queues",
      "definition": {
        "dead-letter-exchange": "vehicle.dlx",
        "dead-letter-routing-key": "failed"
      },
      "priority": 0
    }
  ],
  "queues": [
    {
      "name": "vehicle.control.start_rent",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000,
        "x-queue-type": "classic",
        "x-dead-letter-exchange": "vehicle.dlx"
      }
    },
    {
      "name": "vehicle.control.end_rent",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000,
        "x-dead-letter-exchange": "vehicle.dlx"
      }
    },
    {
      "name": "vehicle.control.kill_vehicle",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 5000,
        "x-dead-letter-exchange": "vehicle.dlx"
      }
    },
    {
      "name": "vehicle.realtime.location",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 200000,
        "x-dead-letter-exchange": "vehicle.dlx"
      }
    },
    {
      "name": "vehicle.realtime.status",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 50000,
        "x-dead-letter-exchange": "vehicle.dlx"
      }
    },
    {
      "name": "vehicle.realtime.battery",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 50000,
        "x-dead-letter-exchange": "vehicle.dlx"
      }
    },
    {
      "name": "vehicle.report.maintenance",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000,
        "x-max-length": 20000,
        "x-dead-letter-exchange": "vehicle.dlx"
      }
    },
    {
      "name": "vehicle.report.performance",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000,
        "x-max-length": 20000,
        "x-dead-letter-exchange": "vehicle.dlx"
      }
    },
    {
      "name": "vehicle.dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 2592000000,
        "x-max-length": 50000
      }
    }
  ],
  "exchanges": [
    {
      "name": "vehicle.exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "vehicle.dlx",
      "vhost": "/",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "vehicle.exchange",
      "vhost": "/",
      "destination": "vehicle.control.start_rent",
      "destination_type": "queue",
      "routing_key": "control.start_rent.#",
      "arguments": {}
    },
    {
      "source": "vehicle.exchange",
      "vhost": "/",
      "destination": "vehicle.control.end_rent",
      "destination_type": "queue",
      "routing_key": "control.end_rent.#",
      "arguments": {}
    },
    {
      "source": "vehicle.exchange",
      "vhost": "/",
      "destination": "vehicle.control.kill_vehicle",
      "destination_type": "queue",
      "routing_key": "control.kill_vehicle.#",
      "arguments": {}
    },
    {
      "source": "vehicle.exchange",
      "vhost": "/",
      "destination": "vehicle.realtime.location",
      "destination_type": "queue",
      "routing_key": "realtime.location.#",
      "arguments": {}
    },
    {
      "source": "vehicle.exchange",
      "vhost": "/",
      "destination": "vehicle.realtime.status",
      "destination_type": "queue",
      "routing_key": "realtime.status.#",
      "arguments": {}
    },
    {
      "source": "vehicle.exchange",
      "vhost": "/",
      "destination": "vehicle.realtime.battery",
      "destination_type": "queue",
      "routing_key": "realtime.battery.#",
      "arguments": {}
    },
    {
      "source": "vehicle.exchange",
      "vhost": "/",
      "destination": "vehicle.report.maintenance",
      "destination_type": "queue",
      "routing_key": "report.maintenance.#",
      "arguments": {}
    },
    {
      "source": "vehicle.exchange",
      "vhost": "/",
      "destination": "vehicle.report.performance",
      "destination_type": "queue",
      "routing_key": "report.performance.#",
      "arguments": {}
    },
    {
      "source": "vehicle.dlx",
      "vhost": "/",
      "destination": "vehicle.dlq",
      "destination_type": "queue",
      "routing_key": "",
      "arguments": {}
    }
  ]
}