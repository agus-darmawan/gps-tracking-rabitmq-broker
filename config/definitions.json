  ],
  "permissions": [
    {
      "user": "tracking_admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "gps_device",
      "vhost": "/",
      "configure": "",
      "write": "gps.tracking.*",
      "read": ""
    },
    {
      "user": "backend_consumer",
      "vhost": "/",
      "configure": "",
      "write": "",
      "read": "gps.tracking.*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "tracking-cluster"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "tracking-ha-policy",
      "pattern": "gps.tracking.*",
      "apply-to": "queues",
      "definition": {
        "ha-mode": "exactly",
        "ha-params": 2,
        "ha-sync-mode": "automatic",
        "message-ttl": 86400000,
        "max-length": 100000,
        "overflow": "reject-publish"
      },
      "priority": 1
    },
    {
      "vhost": "/",
      "name": "dlq-policy",
      "pattern": ".*",
      "apply-to": "queues",
      "definition": {
        "dead-letter-exchange": "gps.tracking.dlx",
        "dead-letter-routing-key": "failed"
      },
      "priority": 0
    }
  ],
  "queues": [
    {
      "name": "gps.tracking.location.raw",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 100000,
        "x-queue-type": "classic",
        "x-dead-letter-exchange": "gps.tracking.dlx"
      }
    },
    {
      "name": "gps.tracking.location.processed",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 172800000,
        "x-max-length": 50000,
        "x-dead-letter-exchange": "gps.tracking.dlx"
      }
    },
    {
      "name": "gps.tracking.device.status",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 50000,
        "x-dead-letter-exchange": "gps.tracking.dlx"
      }
    },
    {
      "name": "gps.tracking.trip.start",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000,
        "x-max-length": 20000,
        "x-dead-letter-exchange": "gps.tracking.dlx"
      }
    },
    {
      "name": "gps.tracking.trip.end",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000,
        "x-max-length": 20000,
        "x-dead-letter-exchange": "gps.tracking.dlx"
      }
    },
    {
      "name": "gps.tracking.dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 2592000000,
        "x-max-length": 50000
      }
    }
  ],
  "exchanges": [
    {
      "name": "gps.tracking.exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "gps.tracking.dlx",
      "vhost": "/",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "gps.tracking.exchange",
      "vhost": "/",
      "destination": "gps.tracking.location.raw",
      "destination_type": "queue",
      "routing_key": "location.raw.#",
      "arguments": {}
    },
    {
      "source": "gps.tracking.exchange",
